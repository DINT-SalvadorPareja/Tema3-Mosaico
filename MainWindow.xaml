<Window x:Class="Tema3_Mosaico.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tema3_Mosaico"
        mc:Ignorable="d"
        Title="Mosaicos" MinHeight="500" MinWidth="600" Height="450" Width="800" Icon="./Assets/32.ico">
    <DockPanel>
        <TextBlock Background="pink" DockPanel.Dock="Top" Height="50"></TextBlock>
        <TextBlock Background="Pink" Width="50" DockPanel.Dock="Left"></TextBlock>
        <TextBlock Background="Green" Height="50" DockPanel.Dock="Bottom"></TextBlock>
        <TextBlock Background="Green" Width="50" DockPanel.Dock="Right"></TextBlock>
        <DockPanel>
            <TextBlock Background="Orange" Width="50" DockPanel.Dock="Left"></TextBlock>
            <TextBlock Background="Orange" Height="50" DockPanel.Dock="Bottom"></TextBlock>
            <TextBlock Background="RosyBrown" Width="50" DockPanel.Dock="Right"></TextBlock>
            <TextBlock Background="RosyBrown" Height="50" DockPanel.Dock="Top"></TextBlock>
            <DockPanel>
                <TextBlock Background="Black"></TextBlock>
                
            </DockPanel>
        </DockPanel>
    </DockPanel>
</Window>
